FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    minY
    {
        type            symmetry;
    }
    maxY
    {
        type            symmetry;
    }
    minX
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    maxX
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    minZ
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    maxZ
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    hull
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
}
// ************************************************************************* //
