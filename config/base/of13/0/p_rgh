FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    minY
    {
        type            symmetry;
    }
    maxY
    {
        type            symmetry;
    }
    minX
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    maxX
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    minZ
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    maxZ
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    hull
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
    }
}
// ************************************************************************* //
